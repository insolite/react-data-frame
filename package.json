{
  "name": "react-data-frame",
  "version": "0.1.7",
  "private": true,
  "author": "Oleg Krasnikov",
  "readme": "README.md",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "start": "concurrently \"yarn package:watch\" \"yarn storybook:start\"",
    "package:watch": "yarn workspace react-data-frame watch",
